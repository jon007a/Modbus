    font-size: 16px; /* Размер шрифта */
}

QSpinBox::up-button, QSpinBox::down-button {
    background-color: #A9A9A9; /* Темная кнопка для увеличения/уменьшения */
    border: none; /* Убираем обводку */
    width: 16px; /* Ширина кнопок */
    height: 10px; /* Высота кнопок для более аккуратного вида */
    border-radius: 5px; /* Закругленные углы для кнопок */
    margin: 2px; /* Добавляем небольшой отступ между кнопками и spin box */
}

QSpinBox::up-button:hover, QSpinBox::down-button:hover {
    background-color: #B0BEC5; /* Более светлый цвет при наведении */
}

QSpinBox::up-button:pressed, QSpinBox::down-button:pressed {
    background-color: #90A4AE; /* Темный цвет при нажатии */
}

QSpinBox::up-button:default {
    border: none; /* Убираем обводку при нажатии по умолчанию */
}

QSpinBox::down-button:default {
    border: none; /* Убираем обводку при нажатии по умолчанию */
